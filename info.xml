<?xml version="1.0"?>
<extension key="org.bemas.invoicestoexact" type="module">
  <file>invoicestoexact</file>
  <name>BEMAS Send Invoices to Exact</name>
  <description>CiviCRM native extension to send invoices to Exact</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alain Benbassat</author>
    <email>alain@businessandcode.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/ErikHommel/org.bemas.invoicestoexact</url>
    <url desc="Documentation">https://github.com/ErikHommel/org.bemas.invoicestoexact</url>
    <url desc="Support">http://www.businessandcode.eu/</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2017-12-20</releaseDate>
  <version>0.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments>Extension is being developed</comments>
  <civix>
    <namespace>CRM/Invoicestoexact</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Invoicestoexact_Upgrader</upgrader>
</extension>
